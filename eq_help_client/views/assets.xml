<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- only backend assets, not for website -->
    <template id="assets_backend" name="help.online.assets" inherit_id="web.assets_backend">
        <xpath expr="." position="inside">
            <link rel="stylesheet" href="/eq_help_client/static/src/css/eq_help_client.css" type="text/css"/>
            <script type="text/javascript" src="/eq_help_client/static/src/js/eq_help_client.js"></script>
        </xpath>
    </template>

    <!-- Frontend Editor, not for normal Website Guests -->
    <template id="assets_editor" inherit_id="website.assets_editor" name="Editor assets help_client">
        <xpath expr="//link[last()]" position="after">
            <link href="/eq_help_client/static/src/css/eq_help_client_frontend_editor.css" rel="stylesheet" type="text/less"/>
        </xpath>
    </template>
</odoo>